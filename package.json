    "@iconify-json/svg-spinners": "^1.2.2",
    "@iconify-json/vscode-icons": "^1.2.23",
    "@lincy/eslint-config": "^5.8.1",
    "@lincy/stylelint-config": "^2.2.0",
    "@lincy/unocss-base-config": "^2.3.0",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^24.1.0",
    "@types/polyfill-library": "^3.108.4",
