    "@unhead/vue": "^2.0.12",
    "@unocss/reset": "^66.3.3",
    "@vueuse/core": "^13.6.0",
    "@vueuse/router": "^13.6.0",
    "alova": "^3.3.4",
    "better-sqlite3": "^12.2.0",
    "default-passive-events": "^4.0.0",
    "element-plus": "^2.10.4",
    "lodash": "^4.17.21",
    "md-editor-v3": "^5.8.2",
