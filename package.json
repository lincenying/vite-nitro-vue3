{
  "name": "vite-nitro-vue3",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "prepare": "nitro prepare",
    "serve": "cross-env PORT=18412 nitro dev",
    "preview": "cross-env PORT=8412 HOST=127.0.0.1 node .output/server/index.mjs",
    "build:client": "vite build",
    "build:server": "nitro build",
    "build": "npm run build:client && npm run build:server",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "preview:vue": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "vue-tsc --noEmit",
    "lint:css": "stylelint '**/*.{css,scss}' --fix",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@lincy/async-validation": "^1.4.0",
    "@lincy/utils": "^0.7.3",
    "@tanstack/vue-query": "^5.92.9",
    "@tato30/vue-pdf": "^2.0.0",
    "@unhead/vue": "^2.1.4",
    "@vueuse/core": "^14.2.0",
    "@vueuse/router": "^14.2.0",
    "alova": "^3.5.0",
    "better-sqlite3": "^12.6.2",
    "default-passive-events": "^4.0.0",
    "destr": "^2.0.5",
    "element-plus": "^2.13.2",
    "highlight.js": "^11.11.1",
    "lodash": "^4.17.23",
    "markdown-it-link-attributes": "^4.0.1",
    "md-editor-v3": "^6.3.1",
    "mitt": "^3.0.1",
    "nitropack": "^2.13.1",
    "ofetch": "^1.5.1",
    "pinia": "^3.0.4",
    "qs": "^6.14.1",
    "store2": "^2.14.4",
    "twig": "^1.17.1",
    "vue": "^3.5.28",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@iconify-json/carbon": "^1.2.18",
    "@iconify-json/fad": "^1.2.1",
    "@iconify-json/svg-spinners": "^1.2.4",
    "@iconify-json/vscode-icons": "^1.2.40",
    "@lincy/eslint-config": "^6.4.0",
    "@lincy/stylelint-config": "^2.2.1",
    "@lincy/unocss-base-config": "^2.4.2",
    "@types/markdown-it-link-attributes": "^3.0.5",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^25.2.2",
    "@types/polyfill-library": "^3.108.4",
    "@types/qs": "^6.14.0",
    "@types/twig": "^1.12.17",
    "@unocss/eslint-plugin": "^66.6.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitejs/plugin-vue-jsx": "^5.1.4",
    "@vue-macros/define-props": "^3.1.2",
    "@vue-macros/define-props-refs": "^3.1.2",
    "@vue-macros/reactivity-transform": "^3.1.2",
    "@vue-macros/volar": "^3.1.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.4.0",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "mockjs": "^1.1.0",
    "polyfill-library": "^4.8.0",
    "sass-embedded": "^1.97.3",
    "stylelint": "^17.1.1",
    "typescript": "~5.9.3",
    "unocss": "^66.6.0",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-icons": "^23.0.1",
    "unplugin-vue-components": "^31.0.0",
    "vite": "^7.3.1",
    "vite-plugin-inspect": "^11.3.3",
    "vite-plugin-progress": "^0.0.7",
    "vue-macros": "^3.1.2",
    "vue-tsc": "^3.2.4"
  },
  "resolutions": {
    "h3": "1.15.4"
  },
  "simple-git-hooks[-del]": {
    "pre-commit": "npm run lint-staged"
  },
  "lint-staged[-del]": {
    "*.{[jt]s?(x),vue,md}": [
      "eslint --fix"
    ],
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
